@page
@model SqlServerWeb.Pages.SalesTaxByStateViewModel
@{
    @ViewData["Title"];
}

 <!DOCTYPE html>
 <html>
    <head>
        <meta name="viewport" content="width=device-width" />
        <title>Index</title>
    </head>
    <body>
        <h4>Customers</h4>
        <hr />
        <table cellpadding="0" cellspacing="0">
            <tr>
                <th>Tax Rate Name</th>
                <th>Tax Rate</th>
                <th>State Name</th>
            </tr>
            @foreach (TaxRateByState taxRate in Model.salesTaxModel)
            {
                <tr>
                    <td>@taxRate.Name</td>
                    <td>@taxRate.TaxRate</td>
                    <td>@taxRate.StateName</td>
                </tr>
            }
        </table>
    </body>
</html>


